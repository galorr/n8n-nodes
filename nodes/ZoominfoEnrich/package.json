{
  "name": "zoominfo-enrich",
  "version": "1.0.0",
  "description": "n8n node to enrich contact and company data using ZoomInfo API",
  "keywords": [
    "n8n-community-node-package",
    "zoominfo",
    "enrich",
    "contact",
    "company"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "n8n",
    "email": "info@n8n.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/n8n-io/n8n.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write . --ignore-path ../../.prettierignore",
    "lint": "eslint . --quiet --fix",
    "lintStaged": "lint-staged",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ZoomInfoApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ZoominfoEnrich/ZoominfoEnrich.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "typescript": "~4.9.5"
  }
}
